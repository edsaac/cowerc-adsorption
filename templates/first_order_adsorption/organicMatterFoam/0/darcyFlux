FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      darcyFlux;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 -3.68e-5);

boundaryField
{
    "(front|back|right|left)"
    {
        type    empty;
    }

    "(top|bottom)"
    {
        type            zeroGradient;
    }
}

